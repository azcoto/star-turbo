{
  "extends": "tsconfig/base.json",

  "compilerOptions": {
      "target": "ES2022",
      "module": "CommonJS",
      "types": ["node", "express", "vite/client"],
      "paths": {
        "@/*": ["./src/*"]
      },
      "resolveJsonModule": true,
      "outDir": "./dist",
  },
  "buildOptions": {
    "incremental": true
  }
}